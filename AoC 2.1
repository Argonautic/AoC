keypad = [[1,2,3],
          [4,5,6],
          [7,8,9]]

actions = {'U':(-1,0),'D':(1,0),'R':(0,1),'L':(0,-1)}

def get_password(input):
    code = []
    input += " "
    position = [1,1]
    for letter in input:
        if letter != ' ':
            position[0] += actions[letter][0] #refactored in next example
            position[1] += actions[letter][1]
            if position[0] > 2: #also this
                position[0] -= 1
            elif position[0] < 0:
                position[0] += 1
            if position[1] > 2:
                position[1] -= 1
            elif position[1] < 0:
                position[1] += 1
        else:
            digit = keypad[position[0]][position[1]]
            code.append(digit)
    return code

