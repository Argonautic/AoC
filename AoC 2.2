keypad = [['','',1,'',''],
          ['',2,3,4,''],
          [5,6,7,8,9],
          ['','A','B','C',''],
          ['','','D','','']]

actions = {'U':(-1,0),'D':(1,0),'R':(0,1),'L':(0,-1)}

def get_password(input):
    code = []
    input += " "
    position = [2,0]
    placeholder = [2,0]
    for letter in input:
        if letter != ' ':
            for value in range(2):
                placeholder[0],placeholder[1] = position[0],position[1]
                position[value] += actions[letter][value]
                if position[0] > 4 or position[0] < 0 \  #can thjs be in a try block?
                or position[1] > (len(keypad[position[0]]) - 1) \
                or position[1] < 0 or keypad[position[0]][position[1]] == '':
                    position[0],position[1] = placeholder[0],placeholder[1]
        else:
            digit = keypad[position[0]][position[1]]
            code.append(digit)
    return code
